{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [ "./lib", "README.md" ],
        // "includePattern": "",
        "excludePattern": "(node_modules/|docs|img/|.gitignore|.git/)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}